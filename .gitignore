# ============================================
# Multi-Module Lambda Project - .gitignore
# ============================================

# ============================================
# Java / Maven Build Artifacts
# ============================================
# Compiled class files
*.class

# Package files (JAR, WAR, EAR)
*.jar
*.war
*.ear

# Build directories
target/
build/
dist/
out/

# Maven generated files
dependency-reduced-pom.xml
.mvn/wrapper/maven-wrapper.jar
.mvn/timing.properties
.mvn/wrapper/maven-wrapper.properties

# Generated sources
generated/
generated-sources/
generated-test-sources/

# ============================================
# IDE Files
# ============================================
# IntelliJ IDEA
.idea/
*.iml
*.iws
*.ipr

# Eclipse
.classpath
.project
.settings/
bin/

# VS Code
.vscode/
*.code-workspace

# NetBeans
nbproject/
nbbuild/
nbdist/
.nb-gradle/

# ============================================
# Terraform / Infrastructure
# ============================================
# Terraform state and lock files
*.tfstate
*.tfstate.*
*.tfstate.backup
.terraform.lock.hcl

# Terraform directories (IMPORTANT: Excludes large provider binaries)
**/.terraform/*
**/.terraform/providers/**

# Terraform variable files (may contain sensitive data)
*.tfvars
*.tfvars.json
terraform.tfvars
terraform.tfvars.json

# Terraform overrides
override.tf
override.tf.json
*_override.tf
*_override.tf.json

# Terraform CLI config
.terraformrc
terraform.rc

# Terraform crash logs
crash.log
crash.*.log

# ============================================
# AWS / Secrets
# ============================================
# AWS credentials
.aws/
aws-credentials

# Private keys and certificates (SENSITIVE)
*.pem
*.key
*.p12
*.pfx
*.crt
*.cer

# LocalStack volumes
localstack/

# ============================================
# Logs
# ============================================
# Log files
*.log
logs/
*.log.*

# Test logs
test-output/
test-results/

# ============================================
